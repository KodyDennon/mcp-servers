    "mcp",
    "model-context-protocol",
    "supabase",
    "postgresql",
    "postgres",
    "database",
    "sql",
    "ai",
    "claude",
    "cursor",
    "gemini",
    "cline",
    "windsurf",
    "codex",
    "llm",
    "agent",
    "mcp-server",
    "monorepo"
  ],
  "author": "Kody Dennon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KodyDennon/mcp-servers.git",
    "directory": "packages/supabase-db"
  },
  "bugs": {
    "url": "https://github.com/KodyDennon/mcp-servers/issues"
  },
  "homepage": "https://github.com/KodyDennon/mcp-servers#readme",
  "engines": {
    "node": ">=18.0.0"
  },
