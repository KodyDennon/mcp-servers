{
  "name": "mcp-supabase-db",
  "version": "3.2.4",
  "description": "Model Context Protocol (MCP) server for Supabase/PostgreSQL database access with dual-mode operation. Provides AI agents with 35 direct tools AND code execution mode (98% token reduction, privacy-first, streaming). Compatible with Claude Code, Cursor, Gemini CLI, Cline, Windsurf, and more.",
  "type": "module",
  "main": "index.js",
  "bin": {
    "supabase-db-mcp": "index.js"
  },
  "files": [
    "index.js",
    "src/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.ts",
    "examples/**/*",
    "README.md",
    "CHANGELOG.md",
    "CODE_EXECUTION_GUIDE.md",
    "CODE_EXECUTION_ANALYSIS.md",
    "IMPLEMENTATION_COMPLETE.md",
    "RELEASE_NOTES_v3.0.0.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node index.js",
    "build": "npx tsc",
    "test": "npm run build && SUPABASE_URL=http://localhost SUPABASE_SECRET_KEY=dummy_key jest",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "@langchain/community": "^1.0.3",
    "@langchain/core": "^1.0.5",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@supabase/supabase-js": "^2.81.1",
    "dotenv": "^17.2.3",
    "find-up": "^8.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.9.0",
    "pg": "^8.16.3",
    "readline": "^1.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "supabase",
    "postgresql",
    "postgres",
    "database",
    "ai",
    "llm",
    "claude",
    "code-execution",
    "privacy",
    "sql",
    "query-builder",
    "streaming",
    "caching"
  ],
  "author": "Kody Dennon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KodyDennon/mcp-servers.git",
    "directory": "packages/supabase-db"
  },
  "bugs": {
    "url": "https://github.com/KodyDennon/mcp-servers/issues"
  },
  "homepage": "https://github.com/KodyDennon/mcp-servers/tree/main/packages/supabase-db#readme",
  "engines": {
    "node": ">=20.0.0"
  }
}
